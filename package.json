{
  "name": "vscode-tsuml",
  "displayName": "vscode-tsuml",
  "description": "A typescript-uml integration for vscode",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "fsahmad",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsahmad/vscode-tsuml.git"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-tsuml.generateClassDiagram"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "vscode-tsuml.generateClassDiagram",
        "title": "TypeScript UML: Generate Class Diagram"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "tslint": "^4.4.2",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "typescript-collections": "^1.2.3",
    "typescript-uml": "0.1.0"
  }
}
